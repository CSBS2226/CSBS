void setup() {

  Serial.begin(9600);  
  delay(20000);

  Serial.println("AT");  
  delay(3000);  

  Serial.println("AT+CMGF=1");  
  delay(3000);  

  Serial.println("AT+CMGS=\"+ZZxxxxxxxxxx\"");  
  delay(3000);  

  Serial.print("hello world");  
  delay(3000);  

  Serial.write(26);  
  delay(3000);  
}

void loop() {
  delay(1000);
}
